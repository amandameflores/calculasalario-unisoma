package br.amandaflores.unisoma.calculasalario.model.entities;

//TODO mapear classes conforme o desafio unisoma
//TODO Padr√£o de Projetos DTO (Data transfer object)

public class ClientResponse {

    private Client client;
    private String message;

    public ClientResponse(Client client, String message) {
        this.client = client;
        this.message = message;
    }

    public ClientResponse() {
    }

    public Client getClient() {
        return client;
    }

    public void setClient(Client client) {
        this.client = client;
    }

    public String getMessage() {
        return message;
    }

    public void setMessage(String message) {
        this.message = message;
    }
}